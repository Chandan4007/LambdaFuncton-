{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "putObject",
        "Effect": "Allow",
        "Action": [
          "s3:PutObject"
        ],
        "Resource": [
          "arn:aws:s3:::test-sourcebucket1/*"
        ]
      },
      {
        "Sid": "getObject",
        "Effect": "Allow",
        "Action": [
          "s3:GetObject"
        ],
        "Resource": [
          "arn:aws:s3:::test-destinationbucket1/*"
        ]
      }
    ]
  }